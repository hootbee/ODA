{
  "name": "agentica-template-standalone",
  "version": "0.0.2",
  "scripts": {
    "bridge:min": "node -r dotenv/config -r ts-node/register src/mcp/httpBridge.ts",
    "bridge:pw": "node -r dotenv/config -r ts-node/register src/mcp/playwrightBridge.ts",
    "start": "node dist/server.js",

    "dev:pw": "concurrently -k -n pw,api -c green,magenta \"npm:bridge:pw\" \"npm:server\"",
    "dev:sp": "concurrently -k -n sp,api -c cyan,magenta \"npm:bridge:sp\" \"npm:server\"",
    "server": "node -r dotenv/config -r ts-node/register src/server.ts",

    "dev": "concurrently -k -n pw,sp,api -c green,cyan,magenta \"npm:bridge:pw\" \"npm:bridge:sp\" \"npm:server\""
  },
